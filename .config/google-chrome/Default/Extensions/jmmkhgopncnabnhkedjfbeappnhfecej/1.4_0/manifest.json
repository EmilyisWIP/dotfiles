{
   "author": "Sayak Sen",
   "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
   },
   "content_scripts": [ {
      "js": [ "script.js" ],
      "matches": [ "https://duckduckgo.com/*", "https://safe.duckduckgo.com/*" ],
      "run_at": "document_idle"
   } ],
   "description": "DuckItUp integrates search google button and movie rating system. Open-Source",
   "icons": {
      "128": "icon_128.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "DuckDuckGo Enhancer",
   "permissions": [ "activeTab", "https://www.omdbapi.com/" ],
   "short_name": "DuckItUp",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.4"
}
